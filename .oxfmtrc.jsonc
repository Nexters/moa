{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "singleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "printWidth": 80,
  "arrowParens": "always",
  "bracketSpacing": true,
  "experimentalSortImports": {
    "groups": []
  },
  "experimentalTailwindcss": {
    "functions": ["cn"]
  }
}
